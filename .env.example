# Copy this file to .env and fill in your values.

# OpenAI
OPENAI_API_KEY=

# Google Cloud / Firebase
GOOGLE_CLOUD_PROJECT=
GOOGLE_APPLICATION_CREDENTIALS=
FIREBASE_PROJECT_ID=

# CORS
# Comma-separated list of allowed origins. Use * for all origins in dev.
CORS_ORIGINS=*

# Encryption
ENCRYPTION_KEY=

# Admin
ADMIN_USERNAME=
ADMIN_PASSWORD=
# Optionally provide a bcrypt hash instead of plain password
ADMIN_PASSWORD_BCRYPT=
ADMIN_SECRET_KEY=
# Optional: comma separated IPs
ADMIN_IP_WHITELIST=
